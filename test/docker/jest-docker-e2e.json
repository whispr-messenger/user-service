{
	"moduleFileExtensions": ["js", "json", "ts"],
	"rootDir": "..",
	"testEnvironment": "node",
	"testRegex": ".e2e-spec.ts$",
	"test:e2e": "jest --config ./test/jest-e2e.json",
	"transform": {
		"^.+\\.(t|j)s$": "ts-jest"
	},
	"moduleNameMapper": {
		"^src/(.*)$": "<rootDir>/../src/$1"
	},
	"setupFilesAfterEnv": ["<rootDir>/docker/setup-docker-e2e.ts"],
	"testTimeout": 30000
}
